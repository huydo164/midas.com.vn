jQuery(document).ready(function($) {

	jQuery(".tab-panels").after(jQuery(".social-icons.share-icons"));
	// jQuery(".tab-panels").after(jQuery(".tagged_as"));
	jQuery(".md-product-sidebar").html(jQuery("#product-sidebar").html());
	
	/* This is basic - uses default settings */
	
	jQuery("a#single_image").fancybox();
	
	/* Using custom settings */
	
	jQuery("a#inline").fancybox({
		'hideOnContentClick': true
	});

	/* Apply fancybox to multiple items */
	
	jQuery("a.group").fancybox({
		'transitionIn'	:	'elastic',
		'transitionOut'	:	'elastic',
		'speedIn'		:	600, 
		'speedOut'		:	200, 
		'overlayShow'	:	false
	});
	

	// slick slider
	$(document).ready(function(){
	  $('.md-slider').slick({
	  });
	});


	$('.md-slider').slick({
	  infinite: true,
	  slidesToShow: 1,
	  adaptiveHeight: true,
	  autoplay: true,
	  arrows: false,
	  autoplaySpeed: 20000,
	});
	
	$('.slick-c').slick({
		arrows: false
	});

	// slider thu vien anh da thuc hien
	$(document).ready(function(){
	  $('.da-thuc-hien').slick({
	  });
	});

	$('.da-thuc-hien').slick({
		slidesToShow: 3,
		slidesToScroll: 1,
		autoplay: true,
		autoplaySpeed: 2000,
	});




});


