jQuery(document).ready(function($){
    if($("#myBtnContact").length) {
        var url      = window.location.href;
        var inputHide = 'input[type="text"][data-field-id="'+vtInputField+'"]';
        $(inputHide).val(url);

        $("#myBtnContact").click(function(){
            $("#myModalContact").show();
        });
        $(".close").click(function(){
            $("#myModalContact").hide();
        });
    }
});